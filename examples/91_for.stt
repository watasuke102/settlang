fn add(self: i32, i: i32) -> i32 {
  ret self+i
}

fn main() -> i32 {
  let exclusive: i32 | add = 0
  for i in 1..14 {
    exclusive.set(exclusive, i)
  }
  
  let inclusive: i32 | add = 0
  for i in 1..=13 {
    inclusive.set(inclusive, i)
  }

  ret if inclusive == exclusive {
    inclusive
  } else {
    0
  }
}
